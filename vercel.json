{
  "version": 2,
  "buildCommand": "pnpm run build",
  "devCommand": "pnpm run dev",
  "installCommand": "pnpm install",
  "framework": null,
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/send-24h-reminders",
      "schedule": "0 9 * * *"
    }
  ]
}
